{
  "name": "ottrip",
  "version": "0.1.0",
  "main": "index.ts",
  "scripts": {
    "dev": "expo start",
    "typecheck": "tsc --noEmit",
    "lint": "biome check --write .",
    "lint:check": "biome check .",
    "format": "biome format --write .",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "expo:doctor": "pnpm dlx expo-doctor",
    "env:pull": "eas env:pull --environment development",
    "build:dev": "eas build --profile dev",
    "build:prod": "eas build --profile prod",
    "deploy:dev": "eas update --environment=development --branch=dev --auto",
    "deploy:prod": "eas update --environment=production --branch=prod",
    "clean": "rm -rf node_modules && pnpm install",
    "prebuild": "pnpm typecheck && pnpm lint:check"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.3",
    "@expo/metro-runtime": "~5.0.4",
    "@react-navigation/bottom-tabs": "^7.4.4",
    "@react-navigation/material-top-tabs": "^7.3.4",
    "@react-navigation/native": "^7.1.16",
    "@react-navigation/stack": "^7.4.4",
    "@tanstack/react-query": "^5.62.0",
    "axios": "^1.6.8",
    "expo": "^53.0.13",
    "expo-apple-authentication": "~7.2.4",
    "expo-build-properties": "~0.14.8",
    "expo-crypto": "^14.1.5",
    "expo-image-picker": "~16.1.4",
    "expo-secure-store": "~14.2.3",
    "expo-status-bar": "~2.2.3",
    "expo-web-browser": "~14.2.0",
    "jotai": "^2.13.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-big-calendar": "^4.18.4",
    "react-native-calendars": "^1.1313.0",
    "react-native-gesture-handler": "^2.27.2",
    "react-native-pager-view": "^6.8.1",
    "react-native-paper": "^5.14.5",
    "react-native-screens": "^4.11.1",
    "react-native-tab-view": "^4.1.2",
    "react-native-web": "~0.20.0",
    "ts-pattern": "^5.7.1",
    "tslib": "^2.8.1",
    "zod": "^3.25.72"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@biomejs/biome": "1.9.4",
    "@types/react": "~19.0.10",
    "@types/react-dom": "19.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.12.4",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "immer",
          "@react-native-kakao/core",
          "@react-native-kakao/user",
          "@suspensive/react",
          "@tanstack/react-form",
          "colord",
          "es-toolkit",
          "ky",
          "ts-brand",
          "ts-pattern"
        ]
      }
    }
  }
}
